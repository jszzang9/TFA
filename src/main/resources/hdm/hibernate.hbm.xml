<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.marlboro.core.model.dto.UserData" table="TFA_USER">
		<id name="uid" type="string">
			<column name="UID" />
		</id>
		<property name="pid" column="PID" type="string" />
	</class>
	<class name="com.marlboro.core.model.dto.PcidData" table="TFA_PC">
		<id name="pcid" type="string">
			<column name="PCID" />
		</id>
		<property name="lid" column="LID" type="string" />
	</class>
	<class name="com.marlboro.core.model.dto.LidData" table="TFA_LOCATION">
		<id name="lid" type="string">
			<column name="LID" />
		</id>
		<property name="mac" column="MAC" type="string" />
	</class>
</hibernate-mapping>